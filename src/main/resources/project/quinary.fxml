<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="vbox" alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.QuinaryController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="newAdoptionButton" mnemonicParsing="false" onAction="#switchToPrimary" text="Adopt a new animal" />
            <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeApplication" text="Close application" />
         </children>
      </HBox>
   </children>
</VBox>
